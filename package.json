    "build": "pnpm build:unocss && craco build",
    "test": "craco test",
    "lint": "eslint . --fix",
    "commit": "git-cz",
    "guidist:server": "styleguidist server",
    "guidist:build": "styleguidist build"
  },
  "dependencies": {
