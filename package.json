{
  "name": "react-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start:css": "unocss 'src/**/*.tsx' --out-file=src/styles/uno.css --watch",
    "start:web": "craco start",
    "start": "run-p start:*",
    "build:unocss": "unocss 'src/**/*.tsx' --out-file=src/styles/uno.css",
    "build": "pnpm build:unocss && craco build",
    "test": "craco test",
    "lint": "eslint . --fix",
    "commit": "git-cz"
  },
  "dependencies": {
    "@iconify-json/mdi": "^1.1.52",
    "@tanstack/react-query": "^4.29.7",
    "@tanstack/react-table": "^8.8.5",
    "@unocss/reset": "^0.51.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-table": "^7.8.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@clyan/eslint-config-react": "^1.0.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.24",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@unocss/cli": "^0.51.13",
    "@unocss/webpack": "^0.51.13",
    "eslint": "^8.39.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "react-scripts": "5.0.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^4.9.5",
    "unocss": "^0.51.13"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
