{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.quickSuggestions": {
    "strings": true,
    "other": true,
    "comments": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "json5"
  ],
  "cSpell.words": [
    "craco",
    "iconify",
    "tanstack",
    "unocss",
    "Konva",
    "tanstack"
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
